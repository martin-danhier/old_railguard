{
    "cSpell.words": [
        "Railguard",
        "Rasterization",
        "Sfloat",
        "Vulkan",
        "premake",
        "renderpasses",
        "submodule",
        "submodules"
    ],
    "c-cpp-flylint.flexelint.enable": false,
    "c-cpp-flylint.lizard.enable": false,
    "c-cpp-flylint.clang.extraArgs": ["-Wno-pragma-once-outside-header"],
    "c-cpp-flylint.includePaths": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/external/glm",
        "${workspaceFolder}/external/vma",
        "${SDL2_PATH}/include",
        "${workspaceFolder}/external/vk-bootstrap/src",
        "${VULKAN_SDK}/include"
    ],
    "c-cpp-flylint.excludeFromWorkspacePaths": [
        "./external"
    ],
    "c-cpp-flylint.standard": [
        "c++20"
    ],
    "c-cpp-flylint.flawfinder.enable": false,
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        ".vs": false,
        "build": false,
        "bin": false,
        ".vscode": false,
        ".gitmodules": false,
        ".gitattributes": false,
        "Doxyfile": false,
        ".gitignore": false,
        ".idea": false,
        "docs": false,
        "external": false,
        "cmakebuild": false,
        "premake5.lua": false,
        "README.md": false,
        ".clang-format": false
    },
    "c-cpp-flylint.cppcheck.force": true,
    "c-cpp-flylint.cppcheck.enable": false,
    "c-cpp-flylint.defines": [
        "USE_VK_VALIDATION_LAYERS",
        "USE_ADVANCED_CHECKS",
        "DEBUG"
    ],
    "explorerExclude.backup": null,
    "files.associations": {
        "*.html": "html",
        "pylintrc": "ini",
        "atomic": "cpp",
        "deque": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "list": "cpp",
        "map": "cpp",
        "memory": "cpp",
        "utility": "cpp",
        "vector": "cpp",
        "xhash": "cpp",
        "xutility": "cpp",
        "compare": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "limits": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "unordered_map": "cpp",
        "xfacet": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocinfo": "cpp",
        "xlocnum": "cpp",
        "xmemory": "cpp",
        "xstring": "cpp",
        "chrono": "cpp",
        "cstddef": "cpp",
        "iomanip": "cpp",
        "thread": "cpp",
        "cinttypes": "cpp",
        "cstdint": "cpp"
    },
}